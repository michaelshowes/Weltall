{
	"name": "@mshowes/weltall",
	"version": "1.0.4",
	"description": "A CLI for generating React components in Next.js 13 (App Router)",
	"publishConfig": {
		"access": "public"
	},
	"main": "dist/index.js",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"type": "module",
	"scripts": {
		"build": "tsup src/index.ts --format cjs,esm --dts",
		"lint": "tsc",
		"ci": "yarn lint && yarn build",
		"release": "yarn lint && yarn build && changeset publish"
	},
	"bin": {
		"weltall": "./dist/index.js"
	},
	"keywords": [
		"cli",
		"react",
		"next.js",
		"typescript",
		"component",
		"generator"
	],
	"author": "Michael Showes",
	"repository": {
		"type": "git",
		"url": "https://github.com/michaelshowes/weltall.git"
	},
	"license": "MIT",
	"dependencies": {
		"chalk": "^5.2.0",
		"commander": "^10.0.1",
		"figlet": "^1.6.0",
		"fs-extra": "^11.1.1",
		"tsup": "^6.7.0"
	},
	"devDependencies": {
		"@changesets/cli": "^2.26.1",
		"@types/figlet": "^1.5.6",
		"@types/fs-extra": "^11.0.1",
		"@types/inquirer": "^9.0.3",
		"@types/node": "^18.16.3",
		"ts-node": "^10.9.1",
		"typescript": "^5.0.4"
	}
}
